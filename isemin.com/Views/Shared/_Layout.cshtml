<!DOCTYPE html>
<html ng-app="isemin">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Игорь Семин - Разработка ASP.NET MVC, Meteor.js, Ext.js, Backbone.js, ORACLE, MS SQL Server, PostgreSQL</title>
    <link rel="shortcut icon" href="~/favicon.ico" type="image/x-icon">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div id="wrapper">
        <!-- Sidebar -->
        <div id="sidebar-wrapper">
            <div class="avatar">
                <img class="img-circle" src="/Content/images/avatar.png" width="100" height="100" alt="Игорь Семин" />
                <h4 class="avatar-name">Игорь Семин</h4>
            </div>
            <div class="clear"></div>
            <ul class="sidebar-nav">
                <li>
                    <a href="#/projects">Проекты</a>
                </li>
                <li>
                    <a href="#/documentation">Документация</a>
                </li>
                <li>
                    <a href="#/articles">Статьи</a>
                </li>
                <li>
                    <a href="#/contacts">Контакты</a>
                </li>
            </ul>
        </div>
        <!-- /#sidebar-wrapper -->
        <!-- Page Content -->
        <div id="page-content-wrapper">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12">
                        @RenderBody()
                    </div>
                </div>
            </div>
        </div>
        <!-- /#page-content-wrapper -->
    </div>

    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Scripts/angular-route.min.js"></script>
    <script src="~/Scripts/angular-animate.min.js"></script>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/app")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", false)
</body>
</html>
