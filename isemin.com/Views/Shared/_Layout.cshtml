<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Игорь Семин - Разработка ASP.NET MVC, Meteor.js, Ext.js, Backbone.js, ORACLE, MS SQL Server, PostgreSQL</title>
    <link rel="shortcut icon" href="~/favicon.ico" type="image/x-icon">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div id="wrapper">
        <!-- Sidebar -->
        <div id="sidebar-wrapper">
            <div class="avatar">
                <img class="img-circle" src="/Content/images/avatar.png" width="100" height="100" alt="Игорь Семин" />
                <h4 class="avatar-name">Игорь Семин</h4>
            </div>
            <div class="clear"></div>
            <ul class="sidebar-nav">
                <li>
                    <a href="#">Проекты</a>
                </li>
                <li>
                    <a href="#">Документация</a>
                </li>
                <li>
                    <a href="#">Статьи</a>
                </li>
                <li>
                    <a href="#">Контакты</a>
                </li>
            </ul>
        </div>
        <!-- /#sidebar-wrapper -->

        <!-- Page Content -->
        <div id="page-content-wrapper">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12">
                        @RenderBody()
                    </div>
                </div>
            </div>
        </div>
        <!-- /#page-content-wrapper -->
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
